//B. Three Brothers 

// #include<bits/stdc++.h>
#include<iostream>
using namespace std;
int main(){
    int a,b;
    cin>>a>>b;
    //outupt->we have to return the nubmer which is not the part of the inputer
    if(a==1&&b==2){
        cout<<3<<endl;
    }
    else if(a==1 &&b==3){
        cout<<2<<endl;
    }
    else if(a==2 && b==1){
        cout<<3<<endl;
    }
    else if(a==3 && b==1){
        cout<<2<<endl;
    }
    else if(a==2 && b==3){
        cout<<1<<endl;
    }
    else if(a==3 && b==2){
        cout<<1<<endl;
    }
    return 0;
}
//1sec-> 10^8 constraints 
